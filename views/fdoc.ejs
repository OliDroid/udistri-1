<form action="/doctor" method="post" class="form-horizontal">
    <div class="form-group">
      <label for="cedula" class="col-sm-2 control-label">Cedula</label>
      <div class="col-sm-10">
        <input type="number" class="form-control" id="cedula" name="cedula" placeholder="Cedula">
      </div>
    </div>
    <div class="form-group">
      <label for="nombre" class="col-sm-2 control-label">Nombre</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" id="nombre" name="nombre" placeholder="Nombre">
      </div>
    </div>
    <div class="form-group">
      <label for="apellido" class="col-sm-2 control-label">Apellido</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" id="apellido" name="apellido" placeholder="Apellido">
      </div>
    </div>
    <div class="form-group">
      <label for="consultorio" class="col-sm-2 control-label">Consultorio</label>
      <div class="col-sm-10">
        <input type="number" class="form-control" id="consultorio" name="consultorio" placeholder="Consultorio">
      </div>
    </div>
    <div class="form-group">
      <label for="email" class="col-sm-2 control-label">Email</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" id="email" name="email" placeholder="Email">
      </div>
    </div>
    <label for="Especialidad" class="labelz">Especialidad</label>
    <input list="Especialidad">
    <datalist id="Especialidad" defaultValue="Especialidad">
        <option value="Medicina General">
        <option value="Cardiología">
        <option value="Medicina Interna">
        <option value="Dermatología">
        <option value="Rehabilitación Física">
        <option value="psicología">
        <option value="Odontología">
        <option value="Radiología">
    </datalist>
    <br>
    <input type="submit" class="submitformbutton" value="Entrar">
</form>
<form action="/doctor/lista" method="get">
    <input type="submit" class="btn btn-primary" value="ver">
</form>